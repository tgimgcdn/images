{
  "version": 1,
  "include": ["/api/*", "/admin/*"],
  "exclude": ["/images/*"],
  "routes": [
    {
      "routePath": "/api/:path*",
      "mountPath": "/api",
      "method": "",
      "module": ["api/[[path]].js:onRequest"]
    },
    {
      "routePath": "/admin/login.html",
      "mountPath": "/",
      "method": "",
      "module": ["public/admin/login.html"]
    },
    {
      "routePath": "/admin/:path*",
      "mountPath": "/admin",
      "method": "",
      "module": ["admin/[[path]].js:onRequest"]
    }
  ],
  "baseURL": "/"
} 
